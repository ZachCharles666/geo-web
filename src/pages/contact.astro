---
export const prerender = true;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="robots" content="noindex, nofollow" />
    <!-- 无 JS 兜底：默认去 EN Contact -->
    <meta http-equiv="refresh" content="0; url=/en/contact" />
    <link rel="canonical" href="https://geo-max.tech/en/contact" />
    <title>Redirecting… | GEO-Max Contact</title>
  </head>
  <body>
    <script>
      (function () {
        try {
          var lang = (navigator.language || navigator.languages?.[0] || '').toLowerCase();
          var isZh = lang.startsWith('zh');
          window.location.replace(isZh ? "/zh/contact" : "/en/contact");
        } catch (e) {
          window.location.replace("/en/contact");
        }
      })();
    </script>

    <noscript>
      <p>
        Redirecting… If it doesn’t happen, choose your language:
        <a href="/zh/contact">中文联系页</a> |
        <a href="/en/contact">English Contact</a>
      </p>
    </noscript>
  </body>
</html>
