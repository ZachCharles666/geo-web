---
export const prerender = true;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="robots" content="noindex, nofollow" />
    <meta http-equiv="refresh" content="0; url=/en/about" />
    <link rel="canonical" href="https://geo-max.tech/en/about" />
    <title>Redirecting… | GEO-Max About</title>
  </head>
  <body>
    <script>
      (function () {
        try {
          var lang = (navigator.language || navigator.languages?.[0] || '').toLowerCase();
          var isZh = lang.startsWith('zh');
          window.location.replace(isZh ? "/zh/about" : "/en/about");
        } catch (e) {
          window.location.replace("/en/about");
        }
      })();
    </script>

    <noscript>
      <p>
        Redirecting… If it doesn’t happen, choose your language:
        <a href="/zh/about">中文介绍</a> |
        <a href="/en/about">English About</a>
      </p>
    </noscript>
  </body>
</html>
